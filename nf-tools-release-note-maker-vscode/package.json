{
  "name": "nf-tools-release-note-maker-vscode",
  "publisher": "netpyoung",
  "displayName": "nf-tools-release-note-maker-vscode",
  "description": "",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/netpyoung/nf-tools-release-note-maker-vscode.git",
    "directory": "nf-tools-release-note-maker-vscode"
  },
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/ReleaseNote.config.toml"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": [
      {
        "id": "nf-tools-release-note-maker-vscode.ReleaseNoteMaker",
        "title": "NF.Tools.ReleaseNoteMaker",
        "properties": {
          "nf-tools-release-note-maker-vscode.ReleaseNoteMaker.isEnableFeature": {
            "type": "boolean",
            "default": true,
            "description": "Enable or disable"
          },
          "nf-tools-release-note-maker-vscode.ReleaseNoteMaker.commandToPreview": {
            "type": "string",
            "default": "dotnet release-note preview --name PREVIEW_NAME --version PREVIEW_VERSION",
            "description": "Set preview command"
          },
          "nf-tools-release-note-maker-vscode.ReleaseNoteMaker.configFileName": {
            "type": "string",
            "default": "ReleaseNote.config.toml",
            "description": "Set config filename for ReleaseNote"
          }
        }
      }
    ],
    "commands": [
      {
        "command": "nf-tools-release-note-maker-vscode.package.cmdPreview",
        "title": "Preview"
      }
    ],
    "submenus": [
      {
        "id": "nf-tools-release-note-maker-vscode.package.submenu",
        "label": "üå∂Ô∏èReleaseNoteMaker"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "submenu": "nf-tools-release-note-maker-vscode.package.submenu",
          "when": "nf-tools-release-note-maker-vscode.package.isConfigFileExist",
          "group": "navigation"
        }
      ],
      "editor/context": [
        {
          "submenu": "nf-tools-release-note-maker-vscode.package.submenu",
          "when": "nf-tools-release-note-maker-vscode.package.isConfigFileExist",
          "group": "navigation"
        }
      ],
      "nf-tools-release-note-maker-vscode.package.submenu": [
        {
          "command": "nf-tools-release-note-maker-vscode.package.cmdPreview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.19.1",
    "@types/vscode": "^1.106.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  }
}